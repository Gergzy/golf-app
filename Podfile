  pod 'Firebase/Database'  # Realtime Database
  pod 'Firebase/Storage'  # Cloud Storage
  pod 'Firebase/Messaging'  # Push Notifications
  pod 'Firebase/Crashlytics'
  # Pods for GolfGPSApp

  target 'GolfGPSAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GolfGPSAppUITests' do
    # Pods for testing
  end

end
